#!/bin/bash
#Generated by JetBrains Toolbox 2.9.1.58121 at 2025-10-03T15:42:30

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/carlcaulkett/Applications/WebStorm.app/Contents/MacOS/webstorm" $wait --args "${intellij_args[@]}"