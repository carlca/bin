#!/bin/bash
#Generated by JetBrains Toolbox 3.0.1.59888 at 2025-10-28T19:48:38

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/carlcaulkett/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"